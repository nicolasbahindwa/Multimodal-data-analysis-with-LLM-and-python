
# API Keys
OPENAI_API_KEY=
export ANTHROPIC_API_KEY= 
export TAVILY_API_KEY= 


# Processing settings
MAX_WORKERS=4
BATCH_SIZE=10
MAX_FILE_SIZE_MB=100

# Chunking settings
CHUNK_SIZE=1000
CHUNK_OVERLAP=200

# Embedding settings
EMBEDDING_MODEL=sentence-transformers/all-mpnet-base-v2
EMBEDDING_DIMENSION=768
USE_GPU=false



# Folders
INPUT_FOLDER=data/input
OUTPUT_FOLDER=data/output
TEMP_FOLDER=data/temp

# Local file connector
LOCAL_FILE_PATH=data/raw/local
LOCAL_PROCESSED_PATH=data/output/local_process_files

GOOGLE_DRIVE_FILE_PATH=data/input/googlefile
GOOGLE_DRIVE_PROCESSED_FILE_PATH=data/output/google_process_file

EXTRACT_DIR=data/extracted
LOG_FOLDER=logs

# SQL Database
# Database settings
DB_HOST=localhost
DB_PORT=5432
DB_NAME=pipeline_db
DB_USER=postgres
DB_PASSWORD=
SQL_CONNECTION_STRING=postgresql://user:password@localhost/db

SQL_DATABASE_TIMEOUT=60
SQL_DATABASE_MAX_CONNECTIONS=10
SQL_DATABASE_POOL_RECYCLE=3600
SQL_DATABASE_CONNECTION_TIMEOUT=30
SQL_DATABASE_EXECUTION_TIMEOUT=120

# Google Drive
GOOGLE_DRIVE_TIMEOUT=120
GOOGLE_DRIVE_CACHE_DURATION=300
GOOGLE_CREDENTIALS_PATH=auth/service_account.json
GOOGLE_DRIVE_FOLDER_ID=1 

# Run Configuration
ENV_FILE=.env
CONFIG_DIR=config
OUTPUT_FILENAME=extracted_data.json
INCREMENTAL=true
DEDUPLICATE=true
CLEAR_CACHE=false

# Logging
LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARNING, ERROR, CRITICAL